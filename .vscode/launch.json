{
    "version": "0.2.0",
    "configurations": [
        {
            /* Build and Debug client.exe */
            "name": "Client",
            "type": "cppdbg",
            "request": "launch",
            "cwd": "C:\\Users\\ludvi\\Programming\\MvL",
            "program": "C:\\Users\\ludvi\\Programming\\MvL\\client.exe",
            "stopAtEntry": false,
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "C:\\msys64\\mingw64\\bin\\gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
